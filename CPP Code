#include <iostream>

using namespace std;

int main() {
    int array
    cout<<"Enter Index: ";
    for(int i=0;i<10;i++)
    {
        cin>>array
    }
    int n = 9; // Length of array
    int x = 7; // Element to search for

    int low = 0;
    int high = n - 1;
    int mid;

    while (low <= high) {
        mid = low + (high - low) / 2;

        if (arr[mid] == x) {
            cout << "Element found at index " << mid << endl;
            return 0;
        } else if (arr[mid] < x) {
            low = mid + 1;
        } else {
            high = mid - 1;
        }
    }

    cout << "Element not found in array" << endl;

    return 0;
}
